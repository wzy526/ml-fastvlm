python -c "
import os

# 检查图像文件夹中的实际文件名格式
image_folder = '/perception-hl/zhuofan.xia/data/gqa/images'
actual_files = os.listdir(image_folder)

print(f'图像文件夹中的实际文件数量: {len(actual_files)}')
print('前20个文件名:')
for i, filename in enumerate(actual_files[:20]):
    print(f'{i+1}: {filename}')

print('\n检查文件名格式:')
jpg_files = [f for f in actual_files if f.endswith('.jpg')]
png_files = [f for f in actual_files if f.endswith('.png')]
print(f'JPG文件数量: {len(jpg_files)}')
print(f'PNG文件数量: {len(png_files)}')

# 检查是否有n前缀的文件
n_prefix_files = [f for f in actual_files if f.startswith('n')]
print(f'以n开头的文件数量: {len(n_prefix_files)}')
if n_prefix_files:
    print('前5个n前缀文件:', n_prefix_files[:5])
"